{
  "version": 2,
  "functions": {
    "dist/src/**/*.js": {
      "includeFiles": ["node_modules/.prisma/**", "node_modules/@prisma/**"]
    }
  },
  "builds": [
    {
      "src": "dist/src/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/src/main.js",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS", "HEAD"]
    }
  ]
}
